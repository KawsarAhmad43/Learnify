                "# Solution: SNR Calculation\n",
                "\n",
                "**Objective**: Math check."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "import math\n",
                "\n",
                "alpha_bar_T = 0.006\n",
                "\n",
                "signal = math.sqrt(alpha_bar_T)\n",
                "noise = math.sqrt(1 - alpha_bar_T)\n",
                "\n",
                "print(f\"Signal Coeff: {signal:.4f}\")\n",
                "print(f\"Noise Coeff: {noise:.4f}\")\n",
                "print(\"Ratio (Noise/Signal):\", noise/signal)"
            ]
        }
