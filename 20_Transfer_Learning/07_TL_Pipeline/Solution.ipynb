            "source": [
                "# Solution: LR Schedule\n",
                "\n",
                "**Objective**: Scheduling."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "lrs = []\n",
                "for epoch in range(1, 11):\n",
                "    if epoch == 1:\n",
                "        lr = 1e-5\n",
                "    elif epoch <= 5:\n",
                "        lr = 1e-3\n",
                "    else:\n",
                "        lr = 1e-4\n",
                "    lrs.append(lr)\n",
                "\n",
                "print(f\"Schedule: {lrs}\")"
            ]
        }
    ],
