            "source": [
                "# Solution: KL Formula\n",
                "\n",
                "**Objective**: Computation."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "import numpy as np\n",
                "\n",
                "def kl_loss(mu, var):\n",
                "    return -0.5 * (1 + np.log(var) - mu**2 - var)\n",
                "\n",
                "print(f\"Perfect: {kl_loss(0, 1):.2f}\")\n",
                "print(f\"Shifted: {kl_loss(5, 1):.2f}\")\n",
                "print(f\"Narrow: {kl_loss(0, 0.01):.2f}\")"
            ]
        }
    ],
